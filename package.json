{
    "name": "bokehistory",
    "version": "0.0.1",
    "description": "jocker face",
    "main": "index.js",
    "engines": {
        "node": ">=4.5.0",
        "npm": "^3.0.0"
    },
    "scripts": {
        "clean": "rimraf dist",
        "compile": "better-npm-run compile",
        "lint": "eslint bin build config server src tests",
        "lint:fix": "npm run lint -- --fix",
        "prestart": "yarn",
        "start": "better-npm-run start",
        "dev": "better-npm-run dev",
        "tdd": "./node_modules/.bin/jest test/ --watchAll",
        "test": "./node_modules/.bin/jest test/",
        "coverage": "./node_modules/.bin/jest --coverage",
        "prebuild": "yarn",
        "build": "better-npm-run build && push-dir --dir=dist --branch=gh-pages --cleanup",
        "codecov": "cat coverage/*/lcov.info | codecov"
    },
    "betterScripts": {
        "compile": {
            "command": "node bin/compile",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "dev": {
            "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "build": {
            "command": "npm run clean && npm run compile",
            "env": {
                "NODE_ENV": "production",
                "DEBUG": "app:*"
            }
        },
        "start": {
            "command": "node --max-old-space-size=4096 bin/dev-server",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "test": {
            "command": "node ./node_modules/karma/bin/karma start config/karma.config",
            "env": {
                "NODE_ENV": "test",
                "DEBUG": "app:*"
            }
        }
    },
    "author": "matholic_dev",
    "license": "MIT",
